{
  "name": "large-file-upload-sdk",
  "version": "1.0.1",
  "description": "A SDK for large file upload with chunking, resumable uploads and instant transfer",
  "main": "dist/bundle.js",
  "module": "dist/bundle.js",
  "browser": "dist/bundle.js",
  "types": "dist/types/sdk/FileUploader.d.ts",
  "files": [
    "dist/",
    "src/"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsc -p tsconfig.types.json && webpack --mode production",
    "dev": "webpack serve --mode development",
    "test": "node server/server.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "file-upload",
    "chunked-upload",
    "resumable-upload",
    "large-file",
    "sdk"
  ],
  "author": "aurora-arch-he",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aurora-arch-he/large-file-upload.git"
  },
  "bugs": {
    "url": "https://github.com/aurora-arch-he/large-file-upload/issues"
  },
  "homepage": "https://github.com/aurora-arch-he/large-file-upload#readme",
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@types/node": "^18.19.130",
    "babel-loader": "^10.0.0",
    "express": "^4.18.2",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "worker-loader": "^3.0.8"
  },
  "dependencies": {
    "spark-md5": "^3.0.2"
  }
